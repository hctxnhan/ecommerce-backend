{
  "env": {
    "browser": true,
    "es2021": true,
    "jest": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "linebreak-style": ["error", "windows"],
    "import/extensions": "off",
    "comma-dangle": "off",
    "no-underscore-dangle": "off",
    "object-curly-newline": "off"
  }
}
